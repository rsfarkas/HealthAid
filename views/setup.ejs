        <!-- SETUP FORM -->
<div class="row">
  <div class="col s12">
      <form action="/setup" method="post">
        <% Object.keys(user.phi).forEach(function(key) { %>
            <% if (key != '$init') {%>
        <fieldset class="form-group">
          <div class="checkbox">
            <label>
        <% if (user.phi[key].visible) { %>
                <input type="checkbox" checked="true" data-toggle="toggle" name=<%= key%> id=<%= key %> >
          <%} else {%>
                <input type="checkbox" data-toggle="toggle" name=<%= key%> id=<%= key %> >
          <%} %>
                <%= key %>
              </label>
        </div>
          <%} %>
        <% });
      %>
          </fieldset>
        <button type="submit" class="btn btn-primary nextBtn fr">Next</button>
    </form>
  </div>
  </div><!--row-->

  </main>

